<?xml version="1.0" encoding="utf-8"?>
<!--
    NOTE: The package name below must match the actual package name
    declared in the MusicFX.apk's AndroidManifest.xml.
    Common package names: com.android.musicfx, org.lineageos.audiofx, etc.
-->
<permissions>
    <privapp-permissions package="com.android.musicfx">
        <permission name="android.permission.MODIFY_AUDIO_SETTINGS"/>
        <permission name="android.permission.MEDIA_CONTENT_CONTROL"/>
        <permission name="android.permission.MODIFY_AUDIO_ROUTING"/>
        <permission name="android.permission.CHANGE_COMPONENT_ENABLED_STATE"/>
        <permission name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    </privapp-permissions>
</permissions>
